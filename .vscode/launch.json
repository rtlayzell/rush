{
   "version": "0.2.0",
   "configurations": [
       {
           "name": "(lldb) Launch",
           "type": "cppdbg",
           "request": "launch",
           // Resolved by CMake Tools:
           "program": "${command:cmake.launchTargetPath}",
         //   "args": ["--dump-parse", "~/src/rush-lang/rush/main.rush" ],
           "stopAtEntry": false,
           "cwd": "${workspaceFolder}",
           "externalConsole": false,
           "MIMode": "gdb",
           "setupCommands": [
               {
                   "description": "Enable pretty-printing for gdb",
                   "text": "-enable-pretty-printing",
                   "ignoreFailures": true
               }
           ]
       }
   ]
}
